{
  "name": "electron-react-boilerplate",
  "version": "0.0.1",
  "description": "ElectronJS with ReactJS & MUI boilerplate",
  "scripts": {
    "prod": "npx rimraf dist && webpack --config=./internals/webpack/webpack.prod.js && electron --noDevServer .",
    "debug": "npx rimraf dist && webpack --config=./internals/webpack/webpack.prod.js && electron --inspect-brk --noDevServer .",
    "dev": "npx rimraf dist && webpack-dev-server --hot --host 0.0.0.0 --config=./internals/webpack/webpack.dev.js",
    "build": "npx rimraf dist && webpack --config=./internals/webpack/webpack.prod.js",
    "package": "npx rimraf dist && webpack --config=./internals/webpack/webpack.prod.js",
    "postpackage": "npx rimraf builds && electron-packager ./ Taggr --out=./builds --asar",
    "test": "jest --coverage",
    "debug-test": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "productName": "electron-react-boilerplate",
  "main": "main.js",
  "author": "Alexander Huber",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@material-ui/core": "^4.7.2",
    "@material-ui/icons": "^4.5.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.3.2",
    "electron": "^7.1.5",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "express": "^4.17.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "npm-check-updates": "^4.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-flexbox-grid": "^2.1.2",
    "react-sticky": "^6.0.3",
    "react-test-renderer": "^16.12.0",
    "style-loader": "^1.0.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/enzyme.config.js"
    ]
  }
}
